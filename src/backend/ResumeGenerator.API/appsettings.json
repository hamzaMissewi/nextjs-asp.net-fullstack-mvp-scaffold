{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost;Database=ResumeGeneratorDB;Trusted_Connection=true;MultipleActiveResultSets=true"
  },
  "JWT": {
    "SecretKey": "your-super-secret-jwt-key-here-make-it-long-and-secure",
    "Issuer": "ResumeGenerator.API",
    "Audience": "ResumeGenerator.Client",
    "ExpirationInMinutes": 60
  },
  "OpenAI": {
    "ApiKey": "your-openai-api-key-here",
    "Model": "gpt-4",
    "MaxTokens": 2000
  },
  "Azure": {
    "ClientId": "your-azure-client-id",
    "ClientSecret": "your-azure-client-secret",
    "TenantId": "your-azure-tenant-id"
  },
  "AllowedHosts": "*",
  "Cors": {
    "AllowedOrigins": ["http://localhost:3000", "https://localhost:3000"]
  }
}
